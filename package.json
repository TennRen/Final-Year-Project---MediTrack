{
  "name": "tenn-meditrack",
  "version": "1.0.0",
  "description": "",
  "main": "internal/Services/MedicationService.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix_git": "git rm --cached -r **.js.map && git rm --cached -r *.js.map",
    "fix": "npm i && npm run build",
    "build": "node build.js && npm run output_tsc_version && tsc --project tsconfig.json",
    "local": "npm run build && node -r dotenv/config index.js",
    "quick_local": "node -r dotenv/config index.js",
    "development": "less-watch-compiler public/styles public/styles",
    "typeorm": "ts-node --project tsconfig.json -r dotenv/config ./node_modules/typeorm/cli.js",
    "new_migration": "npm run typeorm migration:generate -- -n",
    "apply_migration": "npm run typeorm migration:run",
    "blank_migration": "npm run typeorm migration:create",
    "revert_migration": "npm run typeorm migration:revert",
    "output_tsc_version": "echo \"tsc version: \" && tsc -v"
  },
  "author": "Tennessee Renvoize",
  "license": "MIT",
  "dependencies": {
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-subdomain": "^1.0.5",
    "jsdom": "^16.2.2",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "superagent": "^5.2.2",
    "typeorm": "^0.2.22",
    "typescript": "^3.9.3",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.4",
    "@types/express": "^4.17.2",
    "@types/jquery": "^3.3.31",
    "@types/mysql": "^2.15.9",
    "@types/node": "^14.0.5",
    "@types/superagent": "^4.1.7",
    "@types/typescript": "^2.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "less": "^3.10.3",
    "less-watch-compiler": "^1.14.6",
    "mocha": "^10.2.0",
    "ts-node": "^8.6.2"
  }
}
